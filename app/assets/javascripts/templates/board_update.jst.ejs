<form>
  <h3><%= board.title %></h3>
  <div>
    Update Title: <input id="board_title_update" type="text" name="board[title]">
  </div>
  <div>
    <hr>
    <h4>Users:</h4>
    <hr>
    <% for(var index in users){ %>
      <%= users[index].username %>
      <% if (users[index].avatar_file_name == null){ %>
      <img id ="avatar-update" src="/assets/missing.jpg">
    <% } else {%>
      <img id ="avatar-update" src="/system/users/avatars/000/000/00<%=users[index].id%>/original/<%= users[index].avatar_file_name %>">
    <%}%>
    <h3 class="x-delete">x</h3>
    <hr>
   <%}%>
  </div>
  <div>
    <button data-id="<%= board.id  %>" data-method="put">Update</button>
  </div>
</form>
